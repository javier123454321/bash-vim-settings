set nocompatible
syntax on

set shortmess+=I
set background=dark
colorscheme peachpuff

"Sets the left hand column to show you the line# and the rel#
set number
set relativenumber

set laststatus=2
set backspace=indent,eol,start
set hidden

set ignorecase
set smartcase
set incsearch

set noerrorbells visualbell t_vb=

map ` :NERDTreeToggle <cr>
